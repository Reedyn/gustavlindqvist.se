{
	"name": "gustavlindqvist.se",
	"version": "3.0.0",
	"description": "Personal website using Eleventy",
	"exports": "./eleventy.js",
	"type": "module",
	"watch": {
		"compile-css": {
			"patterns": [
				"src/_css"
			],
			"extensions": "css",
			"quiet": false,
			"legacyWatch": true,
			"runOnChangeOnly": false
		},
		"copy-compiled-css": {
			"patterns": [
				"src/assets"
			],
			"extensions": "css",
			"quiet": false,
			"legacyWatch": true,
			"runOnChangeOnly": true
		}
	},
	"scripts": {
		"build": "npx @11ty/eleventy",
		"serve": "npx @11ty/eleventy --serve --incremental --ignore-initial",
		"compile-css": "postcss src/_css/main.css -o src/assets/stylesheets/gustavlindqvist.postcss.css",
		"copy-compiled-css": "copyfiles -f src/assets/stylesheets/*.css _site/assets/stylesheets/",
		"watch-css": "npm-watch compile-css",
		"watch-copy-compiled-css": "npm-watch copy-compiled-css"
	},
	"author": "Gustav Lindqvist",
	"license": "MIT",
	"devDependencies": {
		"@eslint/eslintrc": "^3.1.0",
		"@eslint/js": "^9.12.0",
		"autoprefixer": "^10.4.20",
		"copyfiles": "^2.4.1",
		"cssnano": "^7.0.6",
		"eslint": "^9.12.0",
		"eslint-config-prettier": "^9.1.0",
		"globals": "^15.10.0",
		"npm-watch": "^0.13.0",
		"postcss": "^8.4.47",
		"postcss-cli": "^11.0.0",
		"postcss-custom-media": "^11.0.3",
		"postcss-import": "^16.1.0",
		"postcss-import-ext-glob": "^2.1.1",
		"postcss-nesting": "^13.0.0",
		"prettier": "3.3.3",
		"prettier-eslint": "^16.3.0",
		"prettier-plugin-css-order": "^2.1.2"
	},
	"dependencies": {
		"@11ty/eleventy": "^3.0.0",
		"@11ty/eleventy-fetch": "^3.0.0",
		"@11ty/eleventy-img": "^3.0.0",
		"@11ty/eleventy-navigation": "^0.3.3",
		"@11ty/eleventy-plugin-rss": "^2.0.2",
		"@gerhobbelt/markdown-it-checkbox": "^1.2.0-3",
		"@quasibit/eleventy-plugin-schema": "^1.11.1",
		"cheerio": "^1.0.0-rc.12",
		"d3": "^7.4.4",
		"dotenv": "^16.0.1",
		"eleventy-plugin-page-assets": "^0.3.0",
		"exifr": "^7.1.3",
		"fast-xml-parser": "^4.1.3",
		"fs": "^0.0.1-security",
		"lodash": "^4.17.21",
		"luxon": "^3.2.1",
		"markdown-it": "^13.0.1",
		"markdown-it-abbr": "^1.0.4",
		"markdown-it-attrs": "^4.1.4",
		"markdown-it-container": "^3.0.0",
		"markdown-it-custom-block": "^0.1.2",
		"markdown-it-footnote": "^3.0.3",
		"markdown-it-kbd": "^2.2.2",
		"markdown-it-multimd-table": "^4.1.3",
		"markdown-it-sup": "^1.0.0",
		"markdown-it-task-lists": "^2.1.1",
		"moment": "^2.29.4",
		"netlify-plugin-cache": "^1.0.3",
		"node-fetch": "^2.6.9",
		"opml": "^0.4.24",
		"path": "^0.12.7",
		"probe-image-size": "^7.2.3",
		"rss-parser": "^3.12.0",
		"sanitize-html": "^2.10.0",
		"slugify": "^1.6.5",
		"strava-v3": "^2.1.0"
	}
}
